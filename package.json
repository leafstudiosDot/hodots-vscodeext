{
	"name": "hodotsVSCodeExt",
	"displayName": "hodots. for VSCode",
	"description": "A hodots. browsing extension for VSCode",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:hodotsVSCodeExt.helloWorld"
	],
	"main": "./extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "hodots-browser",
					"title": "hodots.",
					"icon": "resources/icon.svg"
				}
			]
		},
		"views": {
			"hodots-browser": [
				{
					"id": "hodots.postsyou",
					"name": "hodots. Posts by You"
				},
				{
					"id": "hodots.account",
					"name": "Account"
				}
			]
		},
		"commands": [
			{
				"command": "hodotsVSCodeExt.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "hodots.switchenv",
				"title": "Switch Environment"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.63.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4",
		"@vscode/test-electron": "^2.0.3"
	}
}